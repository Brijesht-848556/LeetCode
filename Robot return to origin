class Solution {
    public boolean judgeCircle(String moves) {
        int a=0;
        int b=0;
        for(int i=0;i<moves.length();i++)
        { Character c=moves.charAt(i);
            if(c=='U')
                a++;
            if(c=='D')
                a--;
             if(c=='R')
                b++;
             if(c=='L')
                b--;
        }
        if(a==0&&b==0)
            return true;
        else
            return false;
    }
}
